<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/customDialog_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="true"
    android:gravity="center_vertical"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ProgressBar
            android:id="@+id/dialog_progress1"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="152dp"
            tools:layout_editor_absoluteY="247dp" />

        <TextView
            android:id="@+id/dialog_title"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/object_null"
            android:textAlignment="center"
            android:textSize="15pt"
            tools:layout_editor_absoluteX="424dp"
            tools:layout_editor_absoluteY="768dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/dialog_contents"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/object_null"
        android:textAlignment="center"
        android:textSize="10pt"
        tools:layout_editor_absoluteX="424dp"
        tools:layout_editor_absoluteY="768dp" />

    <ProgressBar
        android:id="@+id/dialog_progress2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="339dp" />

</LinearLayout>